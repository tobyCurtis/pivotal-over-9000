{
    "manifest_version": 2,
    "name": "Pivotal Over 9000",
    "version": "0.1.0",
    "description": "Sets the velocity of a pivotal project to over 9000 on page load",
    "browser_action": {
        "default_icon": "security.png"
    },
    "content_scripts": [{
        "js": ["script.js"],
        "matches": ["https://www.pivotaltracker.com/n/projects/*"]
    }]
  }